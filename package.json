{
  "name": "ai-sdk-provider-codex-cli",
  "version": "0.2.0",
  "description": "AI SDK v5 provider for OpenAI Codex CLI with native JSON Schema support",
  "keywords": [
    "ai-sdk",
    "codex",
    "openai",
    "cli",
    "language-model",
    "gpt-5",
    "provider"
  ],
  "homepage": "https://github.com/ben-vargas/ai-sdk-provider-codex-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ben-vargas/ai-sdk-provider-codex-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ben-vargas/ai-sdk-provider-codex-cli/issues"
  },
  "license": "MIT",
  "author": "Ben Vargas",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "validate": "npm run build && npm run typecheck && npm run format && npm run lint && npm run test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/provider-utils": "3.0.3",
    "jsonc-parser": "^3.3.1"
  },
  "optionalDependencies": {
    "@openai/codex": "^0.44.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "20.17.24",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "5.0.14",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "tsup": "8.5.0",
    "typescript": "5.6.3",
    "vitest": "^3.2.4",
    "typescript-eslint": "^8.6.0",
    "zod": "^4.0.17"
  },
  "peerDependencies": {
    "zod": "^3.0.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
